    "def solve(s):\n",
    "    s = s.split(\" \")\n",
    "    a = []\n",
    "\n",
    "    for i in s:\n",
    "        a.append(i.capitalize())\n",
    "    return \" \".join(a)\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    s = input()\n",
    "    result = solve(s)\n",
    "    print(result)"
   ]
