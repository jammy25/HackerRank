    "def decorator(func):\n",
    "    def inner(l):\n",
    "        func(' '.join(['+91', x[-10:-5], x[-5:]]) for x in l)\n",
    "    return inner\n",
    "\n",
    "@decorator\n",
    "def sort_phone(l):\n",
    "    print(*sorted(l), sep='\\n')\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    l = [input() for _ in range(int(input()))]\n",
    "    sort_phone(l) "
   ]
