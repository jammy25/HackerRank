    "from collections import OrderedDict\n",
    "d = OrderedDict()\n",
    "\n",
    "for _ in range(int(input())):\n",
    "    items, price = input().rsplit(\" \", 1)\n",
    "    \n",
    "    if items not in d:\n",
    "        d[items] = int(price)\n",
    "    else:\n",
    "        d[items] += int(price)\n",
    "        \n",
    "for k, v in d.items():\n",
    "    print(k, v)"
   ]
