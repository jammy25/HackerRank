    "n, m = list(map(int, input().split()))\n",
    "x = []\n",
    "\n",
    "for i in range(m):\n",
    "    l = list(map(float, input().split()))\n",
    "    if len(x) != 0:\n",
    "        x = [sum(i) for i in zip(x, l)]\n",
    "    else:\n",
    "        x = l\n",
    "for i in x:\n",
    "    res = i/m\n",
    "    print('%.1f' %res)"
   ]
