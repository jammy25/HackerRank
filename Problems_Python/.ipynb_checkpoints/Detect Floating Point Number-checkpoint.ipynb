    "import re\n",
    "\n",
    "str_pattern = r\"^[+-]?[0-9]*[.][0-9]+$\"\n",
    "\n",
    "T = int(input())\n",
    "matchs = [False] * T\n",
    "\n",
    "for i in range(T):\n",
    "    str = input()\n",
    "    if re.match(str_pattern, str):\n",
    "        matchs[i] = True\n",
    "print(*matchs, sep = \"\\n\")"
   ]
  }
