    "from itertools import product\n",
    "\n",
    "N,M = map(int, input().split())\n",
    "list = []\n",
    "for _ in range(N):\n",
    "        list.append(map(int, input().split()[1:]))\n",
    "        \n",
    "print(max([sum(map(lambda x: x*x, a))%M for a in product(*list)]))"
   ]
  }
