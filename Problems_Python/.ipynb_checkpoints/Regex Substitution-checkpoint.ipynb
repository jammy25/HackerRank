   "source": [
    "import re\n",
    "\n",
    "def subst(match):\n",
    "    if match.group(0) == \"&& \":\n",
    "        return 'and '\n",
    "    if match.group(0) == \"|| \":\n",
    "        return 'or '\n",
    "    raise ValueError\n",
    "    \n",
    "n = int(input())\n",
    "for _ in range(n):\n",
    "    line = input()\n",
    "    print(re.sub(r'((?<= )\\&\\& )|((?<= )\\|\\| )', subst, line))"
   ]
  }
